class Solution
{
public:
    int largest(vector<int> &arr, int n)
    {
        //code here
        int highest=0;
        for(int i=0;i<=n-1;i++){
            if(highest<arr[i]){
                highest= arr[i];
            }
        }
        return highest;
    }
};
